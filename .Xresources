URxvt*font:			-xos4-terminesspowerline-medium-r-normal--12-120-72-72-c-60-*-*,xft:Material Design Icons:size=5:antialias=false,xft:Symbola:antialias=false
URxvt*intensityStyles:		false
URxvt*urgentOnBell:		true
URxvt*saveLines:		9999
URxvt*scrollBar:		false
URxvt*scrollTtyOutput:		false
URxvt*scrollWithBuffer:		true
URxvt*scrollTtyKeypress:	true
URxvt*internalBorder:		11
URxvt*cursorBlink:		true
URxvt*cursorUnderline:		false
URxvt*pointerBlank:		true
URxvt*secondaryScreen:		true
URxvt*secondaryScroll:		false
URxvt*depth:			32
URxvt*perl-ext-common:		default,matcher,clipboard,url-select,keyboard-select,vtwheel,fullscreen
URxvt*url-select.launcher:	google-chrome-stable
URxvt*url-select.underline:	true
URxvt*keysym.C-A-c:		perl:clipboard:copy
URxvt*keysym.C-A-v:		perl:clipboard:paste
URxvt*keysym.M-u:		perl:url-select:select_next
URxvt*keysym.M-Escape:		perl:keyboard-select:activate
URxvt*keysym.M-s:		perl:keyboard-select:search
URxvt*keysym.Home:		\033[1~
URxvt*keysym.End:		\033[4~
URxvt*keysym.KP_Home:		\033[1~
URxvt*keysym.KP_End:		\033[4~
URxvt*keysym.F11:		perl:fullscreen:switch
URxvt*iso14755:			false

Xft.dpi: 96

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

