! Xft settings
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

! special
*.foreground:   #c3c3c3
*.background:   #272727
*.cursorColor:  #c3c3c3

! black
*.color0:       #5d5d5d
*.color8:       #818181

! red
*.color1:       #ac4142
*.color9:       #bc6667

! green
*.color2:       #90a959
*.color10:      #a6ba7a

! yellow
*.color3:       #f4bf75
*.color11:      #f6cb90

! blue
*.color4:       #8ab4f8
*.color12:      #a1c3f9

! magenta
*.color5:       #aa759f
*.color13:      #bb90b2

! cyan
*.color6:       #75b5aa
*.color14:      #90c3bb

! white
*.color7:       #d0d0d0
*.color15:      #e0e0e0
